From e7abe76f6cc46aabf4dbf99c4758b2f68127865d Mon Sep 17 00:00:00 2001
From: Konsta <konsta09@gmail.com>
Date: Sat, 28 Sep 2013 13:20:52 +0300
Subject: [PATCH] add roamer2 to config.sh

---
 config.sh |    7 +++++++
 1 file changed, 7 insertions(+)

diff --git a/config.sh b/config.sh
index bbb0c50..88e5843 100755
--- a/config.sh
+++ b/config.sh
@@ -115,6 +115,12 @@ case "$1" in
 	repo_sync $1
 	;;
 
+"roamer2")
+	echo DEVICE=roamer2 >> .tmp-config &&
+	echo LUNCH=roamer2-userdebug >> .tmp-config &&
+	repo_sync inari
+	;;
+
 "otoro"|"unagi"|"keon"|"inari"|"leo"|"hamachi"|"peak"|"helix"|"wasabi"|"flatfish")
 	echo DEVICE=$1 >> .tmp-config &&
 	repo_sync $1
@@ -180,6 +186,7 @@ case "$1" in
 	echo - nexus-s
 	echo - nexus-s-4g
 	echo - flo "(Nexus 7 2013)"
+	echo - roamer2
 	echo - otoro
 	echo - unagi
 	echo - inari
